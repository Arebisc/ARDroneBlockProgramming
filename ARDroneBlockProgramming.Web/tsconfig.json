{
  "include": [
    "ClientApp/**/*"
  ],
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es2015",
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "types": ["webpack-env"],
    "lib": ["es2015", "dom"],
    "noImplicitAny": true
  },
  "exclude": [
      "bin",
      "node_modules",
      "./ClientApp/@types/vuedraggable.d.ts"
  ]
}
