{
  "include": [
    "ClientApp/**/*",
    "vue-shims.d.ts"
  ],
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es2015",
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "types": ["webpack-env"],
    "lib": ["es2015", "dom"]
  },
  "exclude": [
      "bin",
      "node_modules"
  ]
}
